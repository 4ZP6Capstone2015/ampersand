CONTEXT OUNLarch

PATTERN OUNLarch
 in :: Dienst -> Proces.
 naam :: Dienst -> Dienstnaam.
 eigenaar :: Dienst * Orgaan [UNI].
 uitvoerder :: Dienst * Orgaan.
 omschrijving :: Dienst -> Dienstomschrijving.
 input :: Dienst * Product.
 output :: Dienst * Product.
 gebruikt :: Dienst * Gegevensverzameling.
 betrokkene :: Dienst * Orgaan.
 systeem :: Dienst -> Systeem.

 beleid :: Proces * Beleidsuitgangspunt.

 deel :: Product * Product.
 applicatie :: Product * Systeem.
 opslag :: Product * Systeem.
 componentvan :: Product * Product.
 categorie :: Product * Productcategorie.

 in :: Gegevensverzameling * Systeem [TOT].
 deel :: Systeem * Systeem.
 deel :: Orgaan  * Orgaan.
 eigenaar :: Systeem * Orgaan [UNI].
 eigenaar :: Gegevensverzameling * Orgaan [UNI].
 concept :: Gegevensverzameling -> Concept.

 naam :: Proces -> Procesnaam.
 eigenaar :: Proces * Orgaan [UNI].
 deel :: Proces  * Proces.
 isa :: Proces * Proces.
 omgeving :: Proces * Orgaan.
ENDPATTERN

  SERVICE signalen : I[ONE]
   = [ "Organen zonder systeem" : -(-V[ONE*Systeem];eigenaar[Systeem*Orgaan])
     , "Systemen zonder eigenaar" : V[ONE*Orgaan]!-eigenaar[Systeem*Orgaan]~
     ]

  SERVICE Gegevensverzameling : I[Gegevensverzameling]
   = [ "in Systeem" : in[Gegevensverzameling*Systeem]
     , eigenaar : eigenaar[Gegevensverzameling*Orgaan]
     , gebruikt : gebruikt~
     ]

  SERVICE Gegevensverzamelingen : I[ONE]
   = [ Gegevensverzamelingen : V[ONE*Gegevensverzameling]
        = [ naam : I[Gegevensverzameling]
          , "systeem" : in[Gegevensverzameling*Systeem]
          , eigenaar : eigenaar[Gegevensverzameling*Orgaan]
          ]
     ]

  SERVICE Systemen : I[ONE]
   = [ Systemen : V[ONE*Systeem]
        = [ naam : I[Systeem]
          , eigenaar : eigenaar
          , gegevens : in[Gegevensverzameling*Systeem]~
          ]
     ]

  SERVICE Systeem : I[Systeem]
   = [ eigenaar : eigenaar
     , "gegevens" : in[Gegevensverzameling*Systeem]~
     , "producten" : opslag~
     , "diensten" : systeem~
     , "bewerkt" : applicatie~
     , "deel van" : deel[Systeem*Systeem]
     , "onderdelen" : deel[Systeem*Systeem]~
     , "wordt gebuikt in" : (gebruikt;in)~
        = [ Dienst : I[Dienst]
          , naam : naam
          ]
     ]

  SERVICE Processen : I[ONE]
   = [ Processen : V[ONE*Proces]
        = [ nr : I[Proces]
          , naam : naam
          , eigenaar : eigenaar
          ]
     ]

  SERVICE Proces : I[Proces]
   = [ naam : naam
     , eigenaar : eigenaar
     , diensten : in~ \/ deel~;in~ \/  deel~;deel~;in~ \/ deel~;deel~;deel~;in~  -- moet worden deel~*;in
        = [ Dienst : I[Dienst]
          , naam : naam
          , eigenaar : eigenaar
          ]

     , beleidsuitgangspunten : beleid
     , omgeving : omgeving
     , "deel van" : deel
     , onderdelen : deel~
     , "is een specifieke vorm van" : isa
     , "specifieke invullingen" : isa~
     ]

  SERVICE Producten : I[ONE]
   = [ Producten : V[ONE*Product]
        = [ nr : I[Product]
          , input : input~
          , output : output~
          , categorie : categorie
          , "deel van" : deel
          ]
     ]

  SERVICE Product : I[Product]
   = [ "categorie" : categorie
     , "bewerken met" : applicatie
     , "opgeslagen in" : opslag
     , "component van" : componentvan
     , "componenten" : componentvan~
     , "deel van" : deel
        = [ Product : I[Product]
          , categorie : categorie
          ]
     , "onderdelen" : deel~
        = [ Product : I[Product]
          , categorie : categorie
          ]
     , "input Dienst" : input~
        = [ Dienst : I[Dienst]
          , naam : naam
          , eigenaar : eigenaar
          , proces : in;naam
          ]
     , "output Dienst" : output~
        = [ Dienst : I[Dienst]
          , naam : naam
          , eigenaar : eigenaar
          , proces : in;naam
          ]
     ]

  SERVICE Organen : I[ONE]
   = [ "Organen en hun eigendom" : V[ONE*Orgaan]
        = [ nr : I[Orgaan]
          , gegevens  : eigenaar[Gegevensverzameling*Orgaan]~
          , systemen  : eigenaar[Systeem*Orgaan]~
          , processen : eigenaar[Proces*Orgaan]~;naam
          , diensten  : eigenaar[Dienst*Orgaan]~;naam
          ]
     ]

  SERVICE Orgaan : I[Orgaan]
   = [ "deel van" : deel
     , "onderdelen" : deel~
     , "eigenaar van Gegevensverzamelingen" : eigenaar[Gegevensverzameling*Orgaan]~
     , "eigenaar van Systemen" : eigenaar[Systeem*Orgaan]~
     , "eigenaar van Processen"  : eigenaar[Proces*Orgaan]~
        = [ Proces : I[Proces]
          , naam : naam
          ]
     , "eigenaar van Diensten"  : eigenaar[Dienst*Orgaan]~
        = [ Dienst : I[Dienst]
          , proces : in
          ]
     , "omgeving Proces" : omgeving~
     , "betrokken bij dienst" : betrokkene~
        = [ Dienst : I[Dienst]
          , proces : in
          ]
     , "uitvoerder van dienst" : uitvoerder~
        = [ Dienst : I[Dienst]
          , proces : in
          ]
     ]

  SERVICE Diensten : I[ONE]
   = [ Diensten : V[ONE*Dienst]
        = [ nr : I[Dienst]
          , naam : naam
          , eigenaar : eigenaar
          , procesnaam : in;naam
          , systeem : systeem
          ]
     ]

  SERVICE Dienst : I[Dienst]
   = [ naam : naam
     , omschrijving : omschrijving
     , eigenaar : eigenaar
     , gebruikt : gebruikt
     , proces : in
        = [ Proces : I[Proces]
          , naam : naam
          ]
     , betrokkenen : betrokkene
     , uitvoerder : uitvoerder
     , input : input
        = [ Product : I[Product]
          , categorie : categorie
          ]
     , output : output
        = [ Product : I[Product]
          , categorie : categorie
          ]
     , systeem : systeem
     ]


